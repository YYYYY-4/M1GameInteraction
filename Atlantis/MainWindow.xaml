<Window x:Class="Atlantis.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Atlantis"
        xmlns:a="clr-namespace:Atlantis"
        xmlns:game="clr-namespace:Atlantis.Game"
        xmlns:scene="clr-namespace:Atlantis.Scene"
        xmlns:menu="clr-namespace:Atlantis.Menus"
        mc:Ignorable="d"
        Title="MainWindow"
        Height="1080"
        Width="1920" WindowState="Maximized" WindowStyle="None">
    <Window.Background>
        <ImageBrush ImageSource="{Binding BackgroundImage}" Stretch="Fill"/>
    </Window.Background>
</Window>
