<local:GameControl x:Class="Atlantis.Game.EndDoor"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Atlantis.Game"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Canvas Width="75" Height="88">
        <Image x:Name="Door" Source="{StaticResource DoorClosed}" Width="75" Height="88"/>
        <local:ShapeDef.ShapeDef>
            <local:ShapeDef EnableSensorEvents="True" IsSensor="True"/>
        </local:ShapeDef.ShapeDef>
    </Canvas>
</local:GameControl>
