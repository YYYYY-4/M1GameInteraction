<local:GameControl x:Class="Atlantis.Game.DeadlyThing"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Atlantis.Game"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Canvas>
        <local:BodyDef.BodyDef>
            <local:BodyDef Type="b2_dynamicBody" />
        </local:BodyDef.BodyDef>
        <Image Source="{StaticResource Fish5_1.png}" Width="50" Height="50"/>

        <Rectangle Width="50" Height="132" Fill="Green" Opacity="0" HorizontalAlignment="Center" VerticalAlignment="Top" Canvas.Top="-27">
            <local:ShapeDef.ShapeDef>
                <local:ShapeDef Category="All" Mask="All" EnableSensorEvents="True" IsSensor="True" />
            </local:ShapeDef.ShapeDef>
        </Rectangle>
    </Canvas>
</local:GameControl>
